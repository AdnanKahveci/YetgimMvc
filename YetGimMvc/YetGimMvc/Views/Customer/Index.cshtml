@model Customer[]

<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">SurName</th>
        <th scope="col">Email</th>
    </tr>
    </thead>
    <tbody>

    @foreach (Customer customer in Model)
    {
        <tr>
            <th scope="row"></th>
            <td>@customer.Name</td>
            <td>@customer.Surname</td>
            <td>@customer.Email</td>
        </tr>
    }
    </tbody>
</table>