@model List<Car>

<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Model</th>
        <th scope="col">Price</th>
        <th scope="col">FuelType</th>
    </tr>
    </thead>
    <tbody>

    @foreach (Car car in Model)
    {
        <tr>
            <th scope="row"></th>
            <td>@car.Name</td>
            <td>@car.Model</td>
            <td>@car.Price</td>
            <td>@car.FuelType</td>
        </tr>
    }
    </tbody>
</table>