@model List<Car>

@{
    double total = Model.Sum(x => x.Price);
    int index = 1;
}

<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Model</th>
        <th scope="col">Price</th>
        <th scope="col">FuelType</th>
    </tr>
    </thead>
    <tbody>
    @foreach (Car car in Model)
    {
        <tr>
            <th scope="row">@index</th>
            <td>@car.Name</td>
            <td>@car.Model</td>
            <td>@car.Price</td>
            <td>@car.FuelType</td>
        </tr>
        index++;
    }
    </tbody>
    <tfoot>
    <tr>
        <td colspan="3"><strong>Total</strong></td>
        <td colspan="2"><strong>@total</strong></td>
    </tr>
    </tfoot>
</table>